{
  "name": "@specs-feup/onnx-flow",
  "version": "1.0.4",
  "publishConfig": {
    "access": "public"
  },
  "description": "Tool to convert an ONNX graph into a data-flow graph (DFG), decomposing its high-level operations into low-level operations and performing a set of optimizations.",
  "bin": {
    "onnx-flow": "./out/src/index.js"
  },
  "keywords": [
    "onnx",
    "optimization",
    "operation decomposition",
    "dfg"
  ],
  "main": "./out/src/index.js",
  "files": [
    "out/"
  ],
  "scripts": {
    "test": "node ./out/test/testing.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "cpx \"src/Onnx/onnx.proto\" \"out/src/Onnx\"",
    "postinstall": "cd node_modules/@specs-feup/flow && tsc -p tsconfig.json --strict false"
  },
  "author": "Nelson Neto, Jos√© Pedro Ferreira",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/specs-feup/onnx-flow.git"
  },
  "homepage": "https://github.com/specs-feup/onnx-flow#readme",
  "license": "Apache-2.0",
  "dependencies": {
    "@specs-feup/flow": "1.0.0",
    "cytoscape": "~3.26.0",
    "cytoscape-fcose": "^2.2.0",
    "express": "^4.21.1",
    "misc-utils-of-mine-typescript": "^0.0.12",
    "node-graphviz": "^0.1.1",
    "onnxruntime-web": "^1.20.1",
    "protobufjs": "^7.4.0",
    "yargs": "^17.7.2"
  },
  "type": "module",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/yargs": "^17.0.33",
    "cpx": "^1.5.0",
    "typescript": "^5.7.2"
  },
  "parserOptions": {
    "project": [
      "./tsconfig.*.json"
    ]
  },
  "exports": {
    "./*": {
      "types": "./out/src/*.d.ts",
      "default": "./out/src/*.js"
    }
  }
}
