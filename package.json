{
  "name": "@specs-feup/onnx2dfg",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Tool to convert an ONNX graph into a data-flow graph (DFG), decomposing its high-level operations into low-level operations and performing a set of optimizations.",
  "bin": {
    "onnx2dfg": "./out/src/index.js"
  },
  "main": "./out/src/index.js",
  "files": [
  "out/"
  ],
  "scripts": {
    "prebuild": "tsc -p tsconfig.prebuild.json",
    "test": "node ./out/test/testing.js",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "author": "Nelson Neto, Jos√© Pedro Ferreira",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/specs-feup/specs-onnx.git"
  },
  "homepage": "https://github.com/specs-feup/specs-onnx#readme",
  "license": "Apache-2.0",
  "dependencies": {
    "@specs-feup/flow": "1.0.0",
    "cytoscape": "~3.26.0",
    "cytoscape-fcose": "^2.2.0",
    "express": "^4.21.1",
    "misc-utils-of-mine-typescript": "^0.0.12",
    "node-graphviz": "^0.1.1",
    "onnxruntime-web": "^1.20.1",
    "protobufjs": "^7.4.0",
    "yargs": "^17.7.2"
  },
  "type": "module",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.7.2"
  },
  "parserOptions": {
    "project": [
      "./*/tsconfig.json",
      "./tsconfig.*.json"
    ]
  },
  "exports": {
    "./*": {
      "types": "./out/src/*.d.ts",
      "default": "./out/src/*.js"
    }
  }
}
