{
  "name": "@specs-feup/onnx2cytoscape",
  "version": "1.0.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Tool to convert an ONNX graph into a Cytoscape graph, using Cytoscape.js and decompose its high-level operations into low-level operations.",
  "bin": {
    "onnx2cytoscape": "./index.js"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@specs-feup/flow": "1.0.0",
    "cytoscape": "~3.26.0",
    "cytoscape-fcose": "^2.2.0",
    "express": "^4.21.1",
    "onnxruntime-web": "^1.20.1",
    "protobufjs": "^7.4.0",
    "ws": "^8.18.0",
    "yargs": "^17.7.2"
  },
  "type": "module",
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "parserOptions": {
    "project": [
      "./*/tsconfig.json",
      "./tsconfig.*.json"
    ]
  },
  "exports": {
    "./*": {
      "types": "./out/src/*.d.ts",
      "default": "./out/src/*.js"
    }
  }
}
