    "style": {
      "background-color": "#8AFFAB"
    }
  },
  {
    "selector": ".constant",
    "style": {
      "background-color": "#FFFFE0"
    }
  },
  {
    "selector": ".output",
    "style": {
      "background-color": "#FF8A8A"
    }
  },
  {
